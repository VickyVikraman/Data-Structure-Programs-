
public class DoublyCircularLikedList {
	Node head;
	class Node
	{
		int data;
		Node prev,next;
		Node(int d)
		{
			data=d;
			prev=next=null;
		}
	}
	public static void main(String ar[])
	{
		DoublyCircularLikedList dc=new DoublyCircularLikedList();
		dc.insert(10);
		dc.insert(20);
		dc.insert(30);
		dc.insert(40);
		dc.insert(50);
		dc.print();
	}
	public void print()
	{
		Node tnode=head;
		do
		{
			System.out.println(tnode.data);
			tnode=tnode.next;
		}while(tnode!=head);
		System.out.println();
		tnode=tnode.prev;
		do
		{
			System.out.println(tnode.data);
			tnode=tnode.prev;
		}while(tnode!=head);
	}
	public void insert(int data)
	{
		if(head==null)
		{
			head=new Node(data);
			head.prev=head.next=head;
			return;
		}
		Node newnode=new Node(data);
		Node tnode=head;
		while(tnode.next!=head)
		{
			tnode=tnode.next;
		}
		head.prev=newnode;
		newnode.next=head;
		tnode.next=newnode;
		newnode.prev=tnode;
	}
}
